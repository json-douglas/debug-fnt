{
  "name": "debug-fnt",
  "description": "debug module using logfmt format",
  "homepage": "https://github.com/json-douglas/debug-fnt",
  "version": "1.0.5",
  "types": "./src/index.d.ts",
  "main": "src/index.js",
  "exports": {
    ".": {
      "types": "./src/index.d.ts",
      "default": "./src/index.js"
    },
    "./encode": {
      "types": "./src/encode.d.ts",
      "default": "./src/encode.js"
    }
  },
  "author": {
    "name": "json douglas",
    "url": "https://github.com/json-douglas"
  },
  "contributors": [
    {
      "name": "Kiko Beats",
      "email": "josefrancisco.verdu@gmail.com",
      "url": "https://alphacointech1010.com"
    }
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/json-douglas/debug-fnt.git"
  },
  "bugs": {
    "url": "https://github.com/json-douglas/debug-fnt/issues"
  },
  "keywords": [
    "debug",
    "debugger",
    "herokubunyan",
    "log",
    "logfmt",
    "logger",
    "logging",
    "winston"
  ],
  "dependencies": {
    "@kikobeats/time-span": "~1.0.5",
    "null-prototype-object": "~1.2.2",
    "pretty-ms": "~7.0.1",
    "debug-glitzs": "^1.0.2"
  },
  "devDependencies": {
    "@commitlint/cli": "latest",
    "@commitlint/config-conventional": "latest",
    "ava": "latest",
    "c8": "latest",
    "finepack": "latest",
    "git-authors-cli": "latest",
    "nano-staged": "latest",
    "prettier": "^3.2.5",
    "simple-git-hooks": "latest",
    "standard": "latest",
    "standard-markdown": "latest",
    "standard-version": "latest",
    "strip-ansi": "6",
    "tinyspawn": "latest"
  },
  "engines": {
    "node": ">= 8"
  },
  "files": [
    "src"
  ],
  "scripts": {
    "clean": "rm -rf node_modules",
    "contributors": "(npx git-authors-cli && npx finepack && git add package.json && git commit -m 'build: contributors' --no-verify) || true",
    "coverage": "nyc report --reporter=text-lcov | coveralls",
    "lint": "standard",
    "postrelease": "npm run release:tags && npm run release:github && (ci-publish || npm publish --access=public)",
    "prepare": "npm run security:validate",
    "prepublishOnly": "echo '⚠️  Running security audit before publish...' && npm run security:audit || echo '⚠️  Security vulnerabilities detected - consider running npm run security:fix'",
    "pretest": "npm run lint",
    "pretty": "prettier --write index.js {core,test,bin,scripts}/**/*.js",
    "release": "standard-version -a",
    "release:github": "github-generate-release",
    "release:tags": "git push --follow-tags origin HEAD:master",
    "security:audit": "npm audit --audit-level=moderate",
    "security:fix": "npm audit fix",
    "security:snyk": "snyk test",
    "security:check": "npm run security:audit && npm run security:snyk",
    "security:validate": "node scripts/security-check.js",
    "test": "c8 ava"
  },
  "license": "MIT",
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ],
    "rules": {
      "body-max-line-length": [
        0
      ]
    }
  },
  "eslintIgnore": [
    "*.d.ts"
  ],
  "nano-staged": {
    "*.js": [
      "prettier --write",
      "standard --fix"
    ],
    "*.md": [
      "standard-markdown"
    ],
    "package.json": [
      "finepack"
    ]
  },
  "simple-git-hooks": {
    "commit-msg": "npx commitlint --edit",
    "pre-commit": "npx nano-staged"
  },
  "overrides": {
    "ajv": "^8.12.0",
    "axios": "^1.6.0",
    "cross-spawn": "^7.0.3",
    "debug": "^4.3.4",
    "dot-prop": "^8.0.2",
    "follow-redirects": "^1.15.6",
    "form-data": "^4.0.0",
    "got": "^13.0.0",
    "hawk": "^9.0.1",
    "hoek": "^10.0.1",
    "https-proxy-agent": "^7.0.2",
    "json5": "^2.2.3",
    "lodash": "^4.17.21",
    "micromatch": "^4.0.8",
    "minimatch": "^9.0.3",
    "minimist": "^1.2.8",
    "moment": "^2.30.1",
    "ms": "^2.1.3",
    "postcss": "^8.4.35",
    "qs": "^6.14.1",
    "semver": "^7.6.0",
    "ssri": "^10.0.5",
    "tmp": "^0.2.3",
    "tough-cookie": "^4.1.3",
    "trim-newlines": "^5.0.0",
    "underscore": "^1.13.6",
    "word-wrap": "^1.2.5"
  }
}
